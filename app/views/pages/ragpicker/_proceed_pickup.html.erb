    <th style='width: 100px'>Required</th>
    <th>Description</th>
  </tr>
  <tr>
    <td>token </td>
    <td>Y</td>
    <td>login token</td>
  </tr>
  <tr>
    <td>id </td>
    <td>Y</td>
    <td>id of pick-up</td>
  </tr>
  <tr>
    <td>line_items[][category_id] </td>
    <td>Y</td>
    <td>id of category</td>
  </tr>
  <tr>
    <td>line_items[][quantity] </td>
    <td>Y</td>
    <td>Quantity</td>
  </tr>
</table>

